<script src="/javascripts/meetingPage.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>

<h1>Create a Meeting!</h1>
<hr>
<form id="createMeeting" class="form-horizontal" role="form">
  <div class="form-group">
    <label class="control-label col-sm-2" for="title">Meeting Title:</label>
    <div class="col-sm-10">
      <input type="title" class="form-control" id="title" placeholder="Enter title">
    </div>
  </div>
  <div class="form-group">
    <label class="control-label col-sm-2" for="location">Location:</label>
    <div class="col-sm-10"> 
      <input type="location" class="form-control" id="location" placeholder="Enter location">
    </div>
  </div>
  <div class="form-group">
    <label class="control-label col-sm-2" for="invitees">Invitees:</label>
    <div class="col-sm-10">
      <input id="state" type="hidden" value="1">
      <input type="invitee" class="form-control" id="invitee1" placeholder="Eg. funnybunny@gmail.com">
    </div><a href="#"></a>
  </div>
  <div class="form-group"> 
    <div class="col-sm-offset-2 col-sm-10">
      <button type="submit" class="btn btn-default">Create Your Meeting</button>
    </div>
  </div>
</form>